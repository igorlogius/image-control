{
  "author": "igorlogius",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "128": "icon.png"
    }
  },
  "description": "control loading of images for domains via whitelist or blacklist",
  "icons": {
    "128": "icon.png"
  },
  "manifest_version": 2,
  "name": "image-load-control",
  "page_action": {
    "browser_style": true,
    "default_icon": "icon.png",
    "default_title": "toggle list state",
    "show_matches": [
      "<all_urls>"
    ],
    "hide_matches": [
      "*://addons.mozilla.org/*"
    ]
  },
  "permissions": [
    "tabs",
    "<all_urls>",
    "webRequest",
    "webRequestBlocking",
    "storage",
    "notifications"
  ],
  "version": "1.4.2"
}
