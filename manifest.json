{
	"description": "Do (not) load images for domains via +white- or blacklist",
	"manifest_version": 2,
	"name": "DontLoadImages",
	"version": "0.2.0",
	"permissions": [
		 "<all_urls>"
		,"webRequest"
		,"webRequestBlocking"
		,"storage"
		,"webNavigation"
		,"tabs"
		,"activeTab"
	], 
	"background": {
		"scripts": ["background.js"]
	},
	"browser_action": {
		"default_icon": {
			"32": "load.png"
		}
	},
	"page_action": {
		 "default_icon": "load.png"
		,"browser_style": true
		,"show_matches": ["<all_urls>"]
	}
}
